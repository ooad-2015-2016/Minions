        <TextBox x:Name="textBox8" Grid.Column="3" Margin="0.333,9.333,34,0" Grid.Row="19" TextWrapping="Wrap" Text="" Grid.ColumnSpan="2" Grid.RowSpan="2"/>
        <DatePicker x:Name="datumrodj" Grid.Column="3" HorizontalAlignment="Stretch" Grid.Row="9" VerticalAlignment="Stretch" Grid.ColumnSpan="2" Margin="0.333,9.666,-136,0.334" Grid.RowSpan="2"/>
        <PasswordBox x:Name="passwordBox" Grid.Column="3" Margin="0.333,0,0,9.667" Grid.Row="16" Grid.RowSpan="2"/>
        <TextBlock x:Name="textBlock10" Grid.ColumnSpan="3" Grid.Column="1" TextWrapping="Wrap" Text="TextBlock" Visibility="Collapsed" Foreground="#FFF00F0F"/>


    </Grid>
</Page>
