        <TextBlock x:Name="textBlock7" Grid.Column="1" Grid.Row="16" TextWrapping="Wrap" Text="Password" Grid.ColumnSpan="2" Margin="0,0,38.667,9.667" Grid.RowSpan="2"/>
        <TextBlock x:Name="textBlock8" Grid.Column="1" Margin="0,0.333,38.667,9.334" Grid.Row="18" TextWrapping="Wrap" Grid.ColumnSpan="2" Grid.RowSpan="2">
        	<Run Text="Zan"/>
        	<Run Text="i"/>
        	<Run Text="manje"/>
        </TextBlock>
        <TextBlock x:Name="textBlock9" Grid.Column="1" Margin="0,9.333,38.667,0" Grid.Row="19" TextWrapping="Wrap" Text="Broj vozačke" Grid.ColumnSpan="2" Grid.RowSpan="2"/>
        <TextBox x:Name="textBox" Grid.Column="3" Margin="0.333,0.333,34,9.334" TextWrapping="Wrap" Text="" Grid.Row="2" Grid.ColumnSpan="2" Grid.RowSpan="2"/>
        <TextBox x:Name="textBox1" Grid.Column="3" Margin="0.333,9.666,34,0" Grid.Row="3" TextWrapping="Wrap" Text="" Grid.ColumnSpan="2" Grid.RowSpan="2"/>
        <TextBox x:Name="textBox2" Grid.Column="3" Margin="0.333,0,34,9.667" Grid.Row="6" TextWrapping="Wrap" Text="" Grid.ColumnSpan="2" Grid.RowSpan="2"/>
