        <TextBlock x:Name="textBlock1" Grid.Column="1" Grid.Row="3" TextWrapping="Wrap" Text="Prezime" Grid.ColumnSpan="2" Margin="0,9.666,38.667,0" Grid.RowSpan="2"/>
        <TextBlock x:Name="textBlock2" Grid.Column="1" Grid.Row="6" TextWrapping="Wrap" Text="Broj telefona" Grid.ColumnSpan="2" Margin="0,0,38.667,9.667" Grid.RowSpan="2"/>
